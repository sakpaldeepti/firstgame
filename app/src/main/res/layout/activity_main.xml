<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/welcome"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CEE3CC"
    android:contentDescription="@null"
    android:foregroundTint="#CCBABA"
    android:scrollbarSize="20dp"
    android:textDirection="firstStrongRtl"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/welcome"
        android:textAlignment="center"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,TextSizeCheck" />

    <TextView
        android:id="@+id/statusBarBackground"
        android:layout_width="336dp"
        android:layout_height="41dp"
        android:layout_margin="80dp"
        android:textAlignment="center"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints,VisualLintOverlap"></TextView>

    <TextView
        android:id="@+id/Xwin"
        android:layout_width="67dp"
        android:layout_height="36dp"
        android:layout_margin="50dp"
        android:textAlignment="center"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints,VisualLintOverlap"></TextView>

    <TextView
        android:id="@+id/Owin"
        android:layout_width="65dp"
        android:layout_height="43dp"
        android:layout_margin="50dp"
        android:textAlignment="center"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints,VisualLintOverlap"></TextView>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/grid"
        tools:ignore="ContentDescription,MissingConstraints" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="405dp"
        android:layout_height="394dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView0"
                android:layout_width="124dp"
                android:layout_height="114dp"
                android:layout_marginTop="0dp"
                android:layout_weight="1"
                android:baselineAlignBottom="false"
                android:onClick="tap"
                android:tag="0"
                android:textAlignment="center"
                tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck" />

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="124dp"
                android:layout_height="114dp"
                android:layout_marginStart="10dp"
                android:layout_weight="1"
                android:baselineAlignBottom="false"
                android:onClick="tap"
                android:tag="1"
                android:textAlignment="center"
                tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="124dp"
                android:layout_height="114dp"
                android:layout_marginStart="10dp"
                android:layout_weight="1"
                android:baselineAlignBottom="false"
                android:onClick="tap"
                android:tag="2"
                android:textAlignment="center"
                tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="123dp"
            android:layout_marginTop="15dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="124dp"
                android:layout_height="114dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:baselineAlignBottom="false"
                android:onClick="tap"
                android:tag="3"
                android:textAlignment="center"
                tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="124dp"
                android:layout_height="114dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:baselineAlignBottom="false"
                android:onClick="tap"
                android:tag="4"
                android:textAlignment="center"
                tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="124dp"
                android:layout_height="114dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:baselineAlignBottom="false"
                android:onClick="tap"
                android:tag="5"
                android:textAlignment="center"
                tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="121dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="124dp"
                android:layout_height="114dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:baselineAlignBottom="false"
                android:onClick="tap"
                android:tag="6"
                android:textAlignment="center"
                tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="124dp"
                android:layout_height="114dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:baselineAlignBottom="false"
                android:onClick="tap"
                android:tag="7"
                android:textAlignment="center"
                tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="124dp"
                android:layout_height="114dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:baselineAlignBottom="false"
                android:onClick="tap"
                android:tag="8"
                android:textAlignment="center"
                tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck" />

        </LinearLayout>
    </LinearLayout>

    <Button
        android:id="@+id/Reset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="gameReset"
        android:text="@string/Reset"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2" />

</androidx.constraintlayout.widget.ConstraintLayout>